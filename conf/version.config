    withName: 'SAMTOOLS_INDEX' {
        container = 'quay.io/biocontainers/samtools:1.19.2--h50ea8bc_1'
    }
    
    withName: 'MULTIQC' {
        container = 'quay.io/biocontainers/multiqc:1.21--pyhdfd78af_0'
    }
}